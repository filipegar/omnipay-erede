{
    "name": "filipegar/omnipay-erede",
    "description": "Rede ecommerce implementation - e.Rede",
    "keywords": [
        "omnipay",
        "erede"
    ],
    "homepage": "https://github.com/filipegar/omnipay-erede",
    "license": "MIT",
    "authors": [
        {
            "name": "Filipe Garcia",
            "email": "epilif09@gmail.com",
            "homepage": "https://filipe.info"
        }
    ],
    "require": {
        "omnipay/common": "~3"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^3",
        "phpro/grumphp": "^0.14"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Erede\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Skeleton\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
